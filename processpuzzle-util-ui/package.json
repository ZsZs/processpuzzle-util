{
  "name": "processpuzzle-util-ui",
  "version": "0.1.0",
  "description": "Utility functions for ProcessPuzzle business platform.",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build-dev": "npm run ci",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts --mode production",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf ./target/coverage ./target/dist ./target/tmp ./target/docs",
    "docs": "compodoc -p tsconfig.json -d ./target/docs --disableCoverage",
    "explorer": "source-map-explorer ./target/dist/index.umd.js",
    "gh-pages": "rimraf ./target/docs && npm run docs && gh-pages -d ./target/docs",
    "lint": "ng lint --type-check --format json > target/issues.json",
    "lint-watch": "ng lint --type-check",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf ./target/dist ./target/tmp",
    "prebuild:watch": "rimraf ./target/dist ./target/tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZsZs/processpuzzle-util.git"
  },
  "author": {
    "name": "Zsolt Zsuffa",
    "email": "zsolt.zsuffa@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZsZs/processpuzzle-util.git/issues"
  },
  "dependencies": {
    "json2typescript": "^1.0.5",
    "rollup-plugin-alias": "^1.4.0"
  },
  "peerDependencies": {
    "@angular/common": "^5.2.10",
    "@angular/core": "^5.2.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@compodoc/compodoc": "^1.1.2",
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/node": "^9.6.5",
    "@types/tapable": "1.0.2",
    "@types/webpack": "^4.1.3",
    "@types/webpack-env": "^1.13.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.0.0",
    "codecov": "^3.0.0",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-expect": "^3.8.3",
    "jasmine-spec-reporter": "~4.2.1",
    "json-loader": "^0.5.7",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "node-sass": "^4.8.3",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.8",
    "protractor": "~5.3.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.10",
    "sass-loader": "^7.0.1",
    "source-map-explorer": "^1.5.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.6.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-cli": "^2.0.14",
    "webpack-rxjs-externals": "^2.0.0",
    "zone.js": "git://github.com/JiaLiPassion/zone.js.git#jasmine-dist"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
