{
  "name": "processpuzzle-util-app",
  "version": "0.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "npm run build-lib-dev && npm run build-app-dev",
    "build-prod": "npm run build-lib-prod && npm run build-app-prod",
    "build-app-dev": "ng build processpuzzle-util-app",
    "build-app-prod": "ng build processpuzzle-util-app",
    "build-lib-dev": "ng build processpuzzle-util-lib --configuration=development",
    "build-lib-prod": "ng build processpuzzle-util-lib --configuration=production",
    "ci": "npm run lint && npm run test && npm run build-prod && npm run npm-pack",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npm-pack": "cd dist/processpuzzle-util-lib && npm pack",
    "test": "npm run test-app && npm run test-lib",
    "test-app": "ng test processpuzzle-util-app --browsers ChromeHeadless --code-coverage",
    "test-app-watch": "ng test processpuzzle-util-app --browsers Chrome --watch",
    "test-lib": "ng test processpuzzle-util-lib --browsers ChromeHeadless --configuration=unit-test --code-coverage",
    "test-lib-watch": "ng test processpuzzle-util-lib --browsers Chrome --watch --configuration=unit-test",
    "package": "npm run build-dev && npm run npm-pack",
    "postversion": "git push && git push --tags",
    "publish": "cd dist/processpuzzle-util-lib && npm publish",
    "start-c9": "ng serve --host 0.0.0.0 --port 8080 --disableHostCheck"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.1",
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "core-js": "^2.5.7",
    "json2typescript": "^1.0.6",
    "ngx-logger": "^3.0.5",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular-devkit/build-ng-packagr": "~0.7.2",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.5.8",
    "codelyzer": "~4.4.3",
    "jasmine-core": "~3.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^3.0.6",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2"
  }
}
