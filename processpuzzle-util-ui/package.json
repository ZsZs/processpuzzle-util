{
  "name": "processpuzzle-util-ui",
  "version": "0.0.7",
  "description": "Utility functions for ProcessPuzzle business platform.",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "processpuzzle"
  ],
  "author": {
    "name": "Zsolt Zsuffa",
    "url": "https://www.linkedin.com/in/zsuffa/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZsZs/processpuzzle-util.git"
  },
  "bugs": {
    "url": "https://github.com/ZsZs/processpuzzle-util/issues"
  },
  "homepage": "https://github.com/ZsZs/processpuzzle-util#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build-dev": "npm run build --dev --environment=dev",
    "build-integ": "npm run build --prod --environment=integ",
    "build-prod": "npm run build --prod --environment=prod",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "tslint --project tsconfig.json --format json --out target/issues.json",
    "lint-watch": "tslint --project tsconfig.json",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start --single-run --code-coverage",
    "test:watch": "karma start --auto-watch --no-single-run --browsers Chrome",
    "test:watch-c9": "karma start --config karma.c9.js --auto-watch --no-single-run",
    "tslint": "tslint"
  },
  "dependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "json2typescript": "^1.0.5"
  },
  "peerDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/core": "^5.2.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@compodoc/compodoc": "^1.0.9",
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/node": "^9.4.7",
    "@types/webpack": "^4.1.0",
    "@types/webpack-env": "^1.13.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^4.0.1",
    "codecov": "^3.0.0",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^3.1.0",
    "jasmine-expect": "^3.8.3",
    "json-loader": "^0.5.7",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.7",
    "sass-loader": "^6.0.7",
    "source-map-explorer": "^1.5.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-angular-externals": "^1.0.2",
    "webpack-cli": "^2.0.12",
    "webpack-rxjs-externals": "^1.1.0",
    "zone.js": "^0.8.20"
  }
}
